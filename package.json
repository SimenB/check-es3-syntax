{
  "name": "check-es3-syntax",
  "version": "1.0.0",
  "description": "Check if contents of a file is es3-compatible",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "del coverage/ .nyc_output/ index.js",
    "compile": "babel check.js -o index.js",
    "cover": "nyc ava",
    "lint": "node-version-gte-4 && eslint . || node-version-lt-4",
    "postcover": "nyc report --reporter=html",
    "posttravis": "nyc check-coverage --lines 100 --functions 100 --branches 100 --stmts 100 ava && nyc report --reporter=lcov",
    "pretravis": "npm run lint",
    "precover": "npm run clean",
    "prepublish": "npm run compile",
    "pretest": "npm run lint",
    "test": "ava",
    "travis": "npm run cover"
  },
  "repository": "SimenB/check-es3-syntax",
  "keywords": [
    "es3",
    "syntax",
    "ie8"
  ],
  "author": "Simen Bekkhus <sbekkhus91@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simenb/check-es3-syntax/issues"
  },
  "homepage": "https://github.com/simenb/check-es3-syntax#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "diff": "^3.0.0",
    "es3ify": "^0.2.2",
    "fs-readdir-recursive": "^1.0.0",
    "lodash.flatten": "^4.3.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-plugin-check-es2015-constants": "^6.8.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.10.1",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.8.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-register": "^6.11.6",
    "codeclimate-test-reporter": "^0.4.0",
    "coveralls": "^2.11.12",
    "del": "^2.2.1",
    "del-cli": "^0.2.0",
    "eslint": "^3.2.2",
    "eslint-config-simenb-ava": "^3.0.0",
    "eslint-config-simenb-base": "^5.0.0",
    "node-version-check": "^2.1.0",
    "nyc": "^8.1.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
